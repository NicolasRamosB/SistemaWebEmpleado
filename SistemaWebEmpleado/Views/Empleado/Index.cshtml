@model IEnumerable<SistemaWebEmpleado.Models.Empleado>

@{
    ViewData["Title"] = "Index";
}
@Html.ActionLink("Nuevo","Create","Empleado")
    <table>
        <tr>
            <th asp-for ="Nombre"></th>
            <th asp-for ="Apellido"></th>
            <th asp-for ="DNI"th>
            <th asp-for ="Legajo"th>
            <th asp-for ="FechaNacimiento"th>
            <th asp-for ="Titulo"th>

            <th></th>
        </tr>
    @foreach (var item in Model)
    {
                <tr>
                <td>
                @Html.DisplayFor(modelItem => item.Nombre)
                </td>
                <td>
                @Html.DisplayFor(modelItem => item.Apellido)
                </td>
                <td>
                @Html.DisplayFor(modelItem => item.DNI)
                </td>
                <td>
                @Html.DisplayFor(modelItem => item.Legajo)
                </td>
                <td>
                @Html.DisplayFor(modelItem => item.FechaNacimiento)
                </td>
                <td>
                @Html.DisplayFor(modelItem => item.Titulo)
                </td>
                <td>
                @Html.ActionLink("Detalle","Details", "Empleado", new{id=item.Id})
                @Html.ActionLink("Eliminar","Delete", "Empleado", new{id=item.Id})
                @Html.ActionLink("Editar","Edit", "Empleado", new{id=item.Id})

                <td/>
                </tr>
    }
    </table>